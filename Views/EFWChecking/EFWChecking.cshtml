@model List<FinalDeliverable_EAD.Models.User>
@{
    ViewBag.title = "WEWChecking";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>EFWChecking</title>
    
</head>
<body>
    <table>
        <tr>
            <td>ID</td>
            <td>Name</td>
        </tr>
        @foreach(var s in Model)
        {
            <tr>
            <td>@s.U_Id</td>
            <td>@s.Name</td>


                <td><a href="/EFWChecking/Update/@s.U_Id">Update</a></td>

                <td><a href="/EFWChecking/Delete/@s.U_Id">Delete</a></td>
             
            </tr>
            
        }
           
    </table>

    <a href="/EFWChecking/Add">AddUsers</a>
    
</body>
</html>
